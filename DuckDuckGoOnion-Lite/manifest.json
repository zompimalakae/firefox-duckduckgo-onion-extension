{
  "manifest_version": 2,
  "name": "DuckDuckGoOnion Lite Search",
  "short_name": "DDGOnion_Lite",
  "description": "Adds the Onion version of DuckDuckGo Lite (Non-JavaScript) as a search option to Firefox.",
  "version": "1.0",
  "icons": {
    "36": "icons/36.png",
    "48": "icons/48.png",
    "72": "icons/72.png",
    "96": "icons/96.png",
    "144": "icons/144.png",
    "192": "icons/192.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "DuckDuckGoOnion Lite",
      "search_url": "https://duckduckgogg42xjoc72x3sjasowoarfbgcmvfimaftt6twagswzczad.onion/lite/?q={searchTerms}",
      "keyword": "@ddgolite",
      "favicon_url": "https://duckduckgogg42xjoc72x3sjasowoarfbgcmvfimaftt6twagswzczad.onion/favicon.ico"
    }
  },
  "author": "zomps",
  "homepage_url": "https://github.com/zompimalakae/firefox-duckduckgo-onion-extension" 
}
